<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Defines the custom XML attributes supported for a HorizontalListView -->
    <declare-styleable name="HorizontalListView">
        <attr name="dividerWidth" format="dimension" />
        <attr name="android:divider" />
        <attr name="android:requiresFadingEdge" />
        <attr name="android:fadingEdgeLength" />
    </declare-styleable>

    <declare-styleable name="RoundCornerProgress">
        <attr name="rcProgress" format="float" />
        <attr name="rcBackgroundPadding" format="dimension" />
        <attr name="rcBackgroundRadius" format="dimension" />
        <attr name="rcMax" format="float" />
        <attr name="rcIconSrc" format="integer" />
        <attr name="rcIconSize" format="dimension" />
        <attr name="rcIconPadding" format="dimension" />
        <attr name="rcHeaderColor" format="color" />
        <attr name="rcProgressColor" format="color" />
        <attr name="rcBackgroundColor" format="color" />
        <attr name="rcTextProgressColor" format="color" />
        <attr name="rcTextProgress" format="string" />
        <attr name="rcTextProgressUnit" format="string" />
        <attr name="rcTextProgressSize" format="dimension" />
        <attr name="rcTextProgressWidth" format="dimension" />
        <attr name="rcTextProgressPadding" format="dimension" />
        <attr name="rcAutoTextChange" format="boolean" />
        <attr name="rcSecondaryProgress" format="float" />
        <attr name="rcSecondaryProgressColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ViewFlow">
        <attr name="sidebuffer" format="integer" />
    </declare-styleable>
    <declare-styleable name="CircleIndicator">
        <attr name="activeColor" format="color" />
        <attr name="inactiveColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="centered" format="boolean" />
        <attr name="fadeOut" format="integer" />
        <attr name="inactiveType">
            <flag name="stroke" value="0" />
            <flag name="fill" value="1" />
        </attr>
        <attr name="activeType">
            <flag name="stroke" value="0" />
            <flag name="fill" value="1" />
        </attr>
        <attr name="circleSeparation" format="dimension" />
        <attr name="activeRadius" format="dimension" />
        <attr name="android:orientation" />
        <attr name="snap" format="boolean" />
        <attr name="stroke" format="boolean" />
        <attr name="strokeColor" format="color" />
        <attr name="strokeWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MoreTextStyle">
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="android:maxLines" />
        <attr name="android:text" />
    </declare-styleable>

    <declare-styleable name="CustomTextView">
        <attr name="boundWidth" format="dimension" />
        <attr name="boundHeight" format="dimension" />
        <attr name="drawableCenter" format="boolean" />
        <attr name="android:text" />
        <attr name="subText" format="string" />
        <attr name="android:textSize" />
        <attr name="subTextSize" format="dimension" />
        <attr name="android:textColor" />
        <attr name="subTextColor" format="color" />
        <attr name="android:checkMark" />
        <attr name="checkMarkBound" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AnimatedExpandableGridView">
        <attr name="horizontalSpacing" format="dimension" />
        <attr name="verticalSpacing" format="dimension" />
        <attr name="stretchMode">
            <enum name="none" value="0"/>
            <enum name="spacingWidth" value="1" />
            <enum name="columnWidth" value="2" />
            <enum name="spacingWidthUniform" value="3" />
        </attr>
        <attr name="columnWidth" format="dimension" />
        <attr name="numColumns" format="integer" min="0">
            <enum name="auto_fit" value="-1" />
        </attr>
        <attr name="itemHeightRatio" format="float" />
    </declare-styleable>

    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer"></attr>
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="BankEditText">
        <attr name="b_separator" format="string"/>
        <attr name="b_customizeMarkerEnable" format="boolean"/>
        <attr name="b_showMarkerTime">
            <enum name="after_input" value="0"/>
            <enum name="before_input" value="1"/>
            <enum name="always" value="2"/>
        </attr>
        <attr name="b_iOSStyleEnable" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="TimeTextView">

    </declare-styleable>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
        <attr name="selectedTabTextColor" format="color" />
        <attr name="pstsUnderlineEqualText" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TempImageView">
        <attr name="animat_id" format="integer" />
    </declare-styleable>
    <declare-styleable name="FocusImageView">
        <attr name="focus_focusing_id" format="integer" />
        <attr name="focus_success_id" format="integer" />
        <attr name="focus_fail_id" format="integer" />
    </declare-styleable>
    <declare-styleable name="Theme">
        <attr name="customImageViewStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="CustomImageView">
        <attr name="circle" format="boolean" />
        <attr name="myCustomRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="EaseChatMessageList">
        <attr name="msgListMyBubbleBackground" format="reference"/>
        <attr name="msgListOtherBubbleBackground" format="reference"/>
        <attr name="msgListShowUserAvatar" format="boolean"/>
        <attr name="msgListShowUserNick" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="EaseConversationList">
        <attr name="cvsListPrimaryTextColor" format="color"/>
        <attr name="cvsListSecondaryTextColor" format="color"/>
        <attr name="cvsListTimeTextColor" format="color"/>
        <attr name="cvsListPrimaryTextSize" format="dimension"/>
        <attr name="cvsListSecondaryTextSize" format="dimension"/>
        <attr name="cvsListTimeTextSize" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="EaseContactList">
        <attr name="ctsListPrimaryTextColor" format="color"/>
        <attr name="ctsListPrimaryTextSize" format="dimension"/>
        <attr name="ctsListShowSiderBar" format="boolean"/>
        <attr name="ctsListInitialLetterBg" format="reference|color"/>
        <attr name="ctsListInitialLetterColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="EaseEmojiconMenu">
        <attr name="bigEmojiconRows" format="integer"/>
        <attr name="emojiconColumns" format="integer"/>
    </declare-styleable>

    <declare-styleable name="EaseChatExtendMenu">
        <attr name="numColumnsEase" format="integer"/>
    </declare-styleable>

    <declare-styleable name="EaseTitleBar">
        <attr name="titleBarTitle" format="string"/>
        <attr name="titleBarLeftImage" format="reference"/>
        <attr name="titleBarRightImage" format="reference"/>
        <attr name="titleBarBackground" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="EaseSwitchButton">
        <attr name="switchOpenImage" format="reference"/>
        <attr name="switchCloseImage" format="reference"/>
        <attr name="switchStatus">
            <enum name="open" value="0"/>
            <enum name="close" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="EaseImageView">
        <attr name="ease_border_color" format="color" />
        <attr name="ease_border_width" format="dimension" />
        <attr name="ease_press_alpha" format="integer" />
        <attr name="ease_press_color" format="color" />
        <attr name="ease_radius" format="dimension" />
        <attr name="ease_shape_type" format="enum">
            <enum name="none" value="0" />
            <enum name="round" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FluidLayout">
        <attr name="gravity" >
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="center" value="0x11" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FluidLayout_Layout">
        <attr name="layout_gravity">
            <flag name="top" value="0x30" />
            <flag name="bottom" value="0x50" />
            <flag name="center" value="0x11" />
        </attr>
    </declare-styleable>

</resources>